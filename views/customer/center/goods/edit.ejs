<script>
    var str = $("#picture").attr("data-url") ? $("#picture").attr("data-url") : "";
    if(str!==null&&str!==undefined&&str!=="") {
        var imgHtml = '<img src="' + str + '" style="width:150px;height:120px" />';
        $("#picture").contents().find('body').html(imgHtml);
    }
    var ue = UE.getEditor('editor',{
        fullscreen:false,
        elementPathEnabled:false,
        toolbars:[['FullScreen',  'Undo', 'Redo',
            '|','bold', 'italic', 'underline',
            '|','forecolor', 'backcolor',
            '|','customstyle', 'paragraph', 'fontfamily', 'fontsize',
            '|','justifyleft', 'justifycenter', 'justifyright', 'justifyjustify',
            '|','simpleupload',

        ]]
    });
    ue.addListener("ready", function () {
        if($(".page").attr("goodsid")){
            $.ajax({
                type:"post",
                url:"/customer/product/edit_content?goodsId=" + $(".page").attr("goodsid"),
                success:function(content){
                    ue.setContent(content.content);
                }
            });
        }
    });
    $(function(){
        $('.time').datetimepicker({
            timepicker: false,
            format:'Y-m-d',
            minDate:false
        });


    })
</script>