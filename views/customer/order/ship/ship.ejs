    <% include ../../header.ejs%>
<%
var paginator = data.paginator || {};
var pageSize = parseInt(paginator.ps) || 10;
var page = parseInt(paginator.p) || 1;
%>
    <div style="position:relative;width:100%;height:100%;">
        <!--Layout-->
        <div class="site-layout">
            <!--Header Toolbar-->
            <% include ../../_nav_top.ejs %>
            <!--Header Toolbar Close-->

            <!--Header-->
            <% include ../../_nav_head.ejs%>
            <!--Header Close-->

            <!--Page Content--><!-- InstanceBeginEditable name="content" -->
            <div class="page">

                <div class="container page-content">
                    <div class="row">
                        <!--Sidebar-->
                        <% var active = "ship"; %>
                        <% include ../../_order_sidebar.ejs %>

                        <!--Catalog List-->
                        <section class="col-md-10 pull-right space-top space-bottom">
                            <div class="filterDivTab">
                                <div class="tabList">
                                    <a href="/order/ship?type=receive"  class="<%= (data.type == "receive" ? "itemActive" : "")%>">待收货
                                    <% var hasCheckedNum= data["oppositeShipInfo"].length>0; %>
                                    <div style="<%= hasCheckedNum?"":'display:none' %>"><div class="checkListCircle"></div></div>
                                    </a>
                                    <a href="/order/ship?type=received" class="<%= (data.type == "received" ? "itemActive" : "")%>">已收货</a>
                                </div>
                                <%
                                var paginator_keywordsValue = '';
                                if (typeof paginator != 'undefined') {
                                    if (typeof paginator.kv != 'undefined') {
                                        paginator_keywordsValue = (paginator.kv == "%") ? "" : paginator.kv;
                                    }
                                }
                                %>
                                <% var placeholder = "请输入发货单号"%>
                                <% var paginatorData = {
                                    url : "ship?",
                                    kv : 'keywordsValueToBeReplaced',
                                    p : 1,
                                    paginator : paginator
                                };
                                %>
                                <% include ../../public/filterInput.ejs%>
                            </div>
                            <table class="sm-page-table">
                                <thead>
                                <tr>
                                    <td><strong>发货单号</strong></td>
                                    <td><strong>订单号</strong></td>
                                    <td><strong>发货时间</strong></td>
                                    <td><strong>笔数/数量/金额</strong></td>
                                    <td><strong>操作</strong></td>
                                </tr>
                                </thead>
                                <tbody>
                                <% if(data.shipInfo.length > 0){%>
                                <% for (var i in data.shipInfo){%>
                                <% var item = data.shipInfo[i]%>
                                <tr>
                                    <td>
                                        <a href="/order/ship/details?shipId=<%=item.shipId%>"><%=item.displayShipId%></a>
                                    </td>
                                    <td>
                                        <a href="/order/detail?orderId=<%=item.orderId%>"><%=item.displayOrderId%></a>
                                    </td>
                                    <td>
                                        <%= item.shipTime %>
                                    </td>
                                    <td>
                                        <%= item.countBatch %>
                                        /
                                        <%= item.quantity %>
                                        /
                                        &#165;<%= item.subtotal || "金额" %>
                                    </td>
                                    <td>
                                        <!-- TODO 根据后台传入的数据判断用户是线付客户还是授信客户 -->
                                        <!--<% if( item.isReceived ==1){%>
                                            <a href="/order/return/apply?shipId=<%=item.shipId%>" class="tooltipped" title="申请退货" data-toggle="tooltip" data-placement="bottom">
                                                <i class="fa fa-truck"></i>
                                            </a>
                                        <% } else if(item.isReceived ==0) {%>
                                            <a href="/order/ship/details?shipId=<%=item.shipId%>" class="tooltipped" title="确认收货" data-toggle="tooltip" data-placement="bottom">
                                                <i class="fa fa-archive"></i>
                                            </a>
                                        <% } %>-->

                                        <% if( item.isReceived ==1){%>
                                        <a href="/order/ship/details?shipId=<%=item.shipId%>" class="tooltipped" title="查看" data-toggle="tooltip" data-placement="bottom">
                                            <i class="fa fa-binoculars"></i>
                                        </a>
                                        <% } else if(item.isReceived ==0) {%>
                                        <a href="/order/ship/details?shipId=<%=item.shipId%>" class="tooltipped" title="确认收货" data-toggle="tooltip" data-placement="bottom">
                                            <i class="fa fa-archive"></i>
                                        </a>
                                        <% } %>
                                    </td>
                                </tr>
                                <% } %>
                                <% } else {%>
                                <tr>
                                    <td colspan="5" style="height: 200px">
                                        <h4 class="blank-content">您目前还没有新的发货信息</h4>
                                    </td>
                                </tr>
                                <% } %>
                                </tbody>
                            </table>
                        </section>
                        <!--Catalog List Close-->

                        <% var url = "ship?"%>
                        <% include ../../_paginator.ejs%>
                    </div>
                </div>
                <section class="specialty-page">
                </section>
            </div>
            <!--Page Content Close-->
        </div><!--Layout Close-->
    <% include ../../_nav_bottom.ejs%>
</div>

<% include ../../footer.ejs %>
